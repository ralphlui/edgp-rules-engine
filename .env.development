# Server Configuration
HOST=localhost
PORT=8090

# Environment
ENVIRONMENT=development

# API Configuration
API_TITLE=EDGP Rules Engine API
API_VERSION=1.0.0
API_DESCRIPTION=Data Quality Validation API using Great Expectations rules

# CORS Configuration
# For development - allows common frontend dev servers
ALLOWED_ORIGINS=["http://localhost:3000","http://localhost:3001","http://localhost:8080","http://localhost:4200","http://localhost:5173","http://127.0.0.1:3000","http://127.0.0.1:8080","http://127.0.0.1:4200","http://127.0.0.1:5173","*"]

# AWS SQS Configuration
# Set these values according to your AWS setup
SQS_AWS_ACCESS_KEY_ID="your key ID"
SQS_AWS_SECRET_ACCESS_KEY="your access key"
SQS_AWS_REGION=ap-southeast-1

# SQS Queue URLs - Configurable templates
SQS_INPUT_QUEUE_URL=SQS_INBOUND_QUEUE
SQS_OUTPUT_QUEUE_URL=SQS_OUTBOUND_QUEUE  

# SQS Processing Configuration - Essential settings only
SQS_MAX_MESSAGES_PER_POLL=10
SQS_WAIT_TIME_SECONDS=20

# Worker Configuration - Essential settings only
SQS_WORKER_COUNT=4
SQS_AUTO_START_WORKERS=true
SQS_MAX_RETRIES=3
SQS_PROCESSING_TIMEOUT=120