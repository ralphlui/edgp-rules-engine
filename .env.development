# Production Server Configuration
HOST=0.0.0.0
PORT=8008

# Environment
ENVIRONMENT=development

# API Configuration
API_TITLE=EDGP Rules Engine API
API_VERSION=1.0.0
API_DESCRIPTION=Data Quality Validation API using Great Expectations rules

# CORS Configuration - Development (More Permissive)
# Allow common development frontend servers
ALLOWED_ORIGINS=["http://localhost:3000","http://localhost:3001","http://localhost:8080","http://localhost:4200","http://localhost:5173","http://127.0.0.1:3000","http://127.0.0.1:8080","http://127.0.0.1:4200","http://127.0.0.1:5173","*"]

# AWS SQS Configuration - Development Environment
# Set these values according to your development AWS setup
SQS_AWS_ACCESS_KEY_ID=your_dev_access_key_here
SQS_AWS_SECRET_ACCESS_KEY=your_dev_secret_key_here
SQS_AWS_REGION=us-east-1
SQS_AWS_SESSION_TOKEN=

# SQS Queue URLs - Development Environment (replace with your actual queue URLs)
SQS_INPUT_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/dev-validation-input-queue
SQS_OUTPUT_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/dev-validation-output-queue
SQS_DLQ_URL=https://sqs.us-east-1.amazonaws.com/123456789012/dev-validation-dlq

# Legacy support (can be removed later)
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/dev-validation-input-queue
SQS_DLQ_URL=https://sqs.us-east-1.amazonaws.com/123456789012/dev-validation-dlq

# SQS Processing Configuration - Development
SQS_MAX_MESSAGES_PER_POLL=5
SQS_VISIBILITY_TIMEOUT=300
SQS_WAIT_TIME_SECONDS=20
SQS_POLL_INTERVAL=10
SQS_MAX_RETRY_DELAY=300

# Worker Configuration - Development (Lower resource usage)
SQS_WORKER_COUNT=2
SQS_AUTO_START_WORKERS=false
SQS_MAX_RETRIES=3
SQS_RETRY_DELAY=30

# Processing Limits - Development
SQS_PROCESSING_TIMEOUT=120
SQS_BATCH_PROCESSING=true

# Health Check - Development
SQS_HEALTH_CHECK_INTERVAL=60
